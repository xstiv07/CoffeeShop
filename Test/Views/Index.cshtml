<div class="container">
    <table class="table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Milk</th>
                <th>Size</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="item in items">
                <td>{{item.Name}}</td>
                <td>{{item.Description}}</td>
                <td>{{item.Milk}}</td>
                <td>{{item.Size}}</td>
                <td>{{item.Price}}</td>
            </tr>
        </tbody>
    </table>
</div>